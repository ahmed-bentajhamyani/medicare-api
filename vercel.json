{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": [
                    "https://medicare-consultations.vercel.app",
                    "http://localhost:4200"
                ]
            }
        }
    ]
}